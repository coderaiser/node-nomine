{
  "name": "nomine",
  "version": "2.0.0",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "file rename as express middleware",
  "homepage": "http://github.com/coderaiser/node-nomine",
  "main": "lib/nomine.js",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-nomine.git"
  },
  "keywords": [
    "file",
    "folder",
    "rename",
    "group",
    "name"
  ],
  "scripts": {
    "test": "tape test/*.js",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "lint:lib": "eslint -c .eslintrc.server lib",
    "lint:test": "eslint test",
    "lint": "redrun lint:*",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "es6-promisify": "^6.0.0",
    "pullout": "^1.0.2",
    "renamify": "^2.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.0.0",
    "eslint-plugin-node": "^6.0.1",
    "express": "^4.14.0",
    "node-fetch": "^2.1.2",
    "nodemon": "^1.11.0",
    "nyc": "^12.0.2",
    "redrun": "^6.0.4",
    "tape": "^4.5.1"
  }
}
